    <buildLocations xsi:type="buildscm:GITBuildLocation" xmi:id="_MqFqEKMYEeWyHtd4CZYYCQ" gitLocation="_tnbX8G3dEeW8Or0zSwVlzA" folderName="massviewpoint/features"/>
    <buildLocations xsi:type="buildscm:GITBuildLocation" xmi:id="_qRhzIG3dEeW8Or0zSwVlzA" suffix="" gitLocation="_tnbX8G3dEeW8Or0zSwVlzA" folderName="massviewpoint/plugins"/>
    <buildLocations xsi:type="buildstep:TargetPlatformBuildLocation" xmi:id="_O5Zu8G3eEeW8Or0zSwVlzA" path="${usedTP}/capella/eclipse"/>
    <components xsi:type="buildstep:Feature" xmi:id="_QaegIG3eEeW8Or0zSwVlzA" id="org.polarsys.capella.vp.mass.feature"/>
  </steps>
  <steps xsi:type="buildstep:PublishStep" xmi:id="_7nYZwG3fEeW8Or0zSwVlzA" name="" description="Publish feature" component="_QaegIG3eEeW8Or0zSwVlzA" generateDropins="true"/>
