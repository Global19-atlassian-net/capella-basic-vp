  <properties xmi:id="_Ehc5IG3dEeW8Or0zSwVlzA" key="usedTP" value="${publish}/BasicMass" type="INLINED"/>
  <properties xmi:id="_3Z3PUm0BEeWZLqZa10vhXw" key="feature.name" value="org.polarsys.capella.vp.mass.feature" type="INLINED"/>
