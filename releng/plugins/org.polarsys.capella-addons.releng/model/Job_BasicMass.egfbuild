    <steps xsi:type="buildfile:RenameStep" xmi:id="_WiwtAG3eEeW8Or0zSwVlzA" name="del-feature-suffix-dropins-dir-name" description="Delete feature suffix from dropins directory name" sourcePath="${publish}/BasicMass/dropins/${feature.name}" destinationPath="${publish}/BasicMass/dropins/BasicMass"/>
